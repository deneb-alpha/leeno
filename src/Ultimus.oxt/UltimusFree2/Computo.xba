Sub genera_variante
	chiudi_dialoghi
	Visualizza_sheet_TUTTE
	If Not ThisComponent.Sheets.hasbyname(&quot;VARIANTE&quot;) Then
		ThisComponent.Sheets.CopybyName(&quot;COMPUTO&quot;, &quot;VARIANTE&quot;, 4)
		sistema_attributi_sheet
		oSheet = ThisComponent.Sheets.getByName(&quot;VARIANTE&quot;)
		
		oSheet.getCellByPosition(2,0).String  = &quot;VARIANTE&quot;
		oSheet.getCellByPosition(2,0).CELLSTYLE = &quot;comp Int_colonna&quot;
		oSheet.getCellRangeByPosition(0,0,42,0).Cellbackcolor = (RGB(255,255,102))
		oSheet.getCellRangeByPosition(0,2,42,2).Cellbackcolor = (RGB(255,255,102))
		ThisComponent.CurrentController.Select(oSheet)
		SetTabColorN(RGB(255,255,102))
		ThisComponent.CurrentController.Select(oSheet.GetCellByPosition(0,0))
		eventi_assegna
	Else
		ThisComponent.CurrentController.Select(ThisComponent.Sheets.getByName(&quot;VARIANTE&quot;))
		unSelect &apos;unselect ranges 	
		Exit Sub
	EndIf
	If ultima_voce = 0 Then
		Exit Sub
	Else
		test = msgbox (&quot;Vuoi svuotare la VARIANTE appena generata?.&quot; &amp; CHR$(10) &amp; CHR$(10) _
		&amp; &quot;Se decidi di continuare, cancellerai tutte le voci di&quot; &amp; CHR$(10) _
		&amp; &quot;misurazione già presenti in questo elaborato.&quot; &amp; CHR$(10) &amp; CHR$(10) _
		&amp; &quot;Cancello le voci di misurazione?&quot; &amp; CHR$(10) &amp; CHR$(10), 4 +32 , &quot;ATTENZIONE!&quot;)
		If test = 7 Then
			Exit Sub
		Else
			oSheet.rows.removeByIndex (3,ultima_voce-2)&apos;cancella tutto il foglio
			ThisComponent.CurrentController.Select(oSheet.GetCellByPosition(0,2))
			nuova_voce_computo_att
		EndIf
	EndIf

End Sub


